doctype html
html(lang='en')
	head
		meta(charset='utf-8')
		meta(http-equiv='x-ua-compatible', content='ie=edge')
		meta(name='viewport', content='width=device-width, initial-scale=1.0')

		title Pokemon React
		// Google Fonts
		link(href='https://fonts.googleapis.com/css?family=Open+Sans', rel='stylesheet', type='text/css')
		// Normalize CSS
		link(href='https://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.3/normalize.min.css', rel='stylesheet', type='text/css')
		// CSS
		link(href='css/main.min.css', rel='stylesheet', type='text/css')

		// React Files
		script(src='https://cdnjs.cloudflare.com/ajax/libs/react/0.14.6/react.min.js')
		script(src='https://cdnjs.cloudflare.com/ajax/libs/react/0.14.6/react-dom.min.js')
		script(src='https://cdnjs.cloudflare.com/ajax/libs/react-router/2.0.0-rc1/ReactRouter.min.js')
		script(src='https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.34/browser.min.js')
		// jQuery
		script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js')
		// React App
		script(type='text/babel', src='js/app.js')
	body
		#container
